{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Book",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "minLength": 3,
      "maxLength": 200
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 3,
      "maxLength": 150
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "books"
    },
    "subjects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subject.subject",
      "mappedBy": "books"
    },
    "originalPublicationDate": {
      "type": "date"
    },
    "summary": {
      "type": "text"
    },
    "editions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::edition.edition"
    }
  }
}
